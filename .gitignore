# Ignore common system files and editor/IDE artifacts
*.swp
*.swo
*.tmp
*.bak
*.log
*.DS_Store
.vscode/
.vscode-server/
.idea/
*.iml
*.editorconfig
.shellcheckrc
.flake8

# Ignore compiled object files and binaries
*.o
*.out
*.elf
*.bin

# Ignore executables and libraries
*.exe
*.dll
*.so
*.dylib

# Ignore capture_app compiled binaries and temporary files
capture_app/*.o
capture_app/*.out
capture_app/*.elf
capture_app/*.bin
capture_app/*.exe
capture_app/.*
capture_app/*.log

# Ignore other specific temporary or generated files
*.a
*.la
*.lo
*.loT
*.rej
*.orig
*.gch
*~

# Ignore Python bytecode files (if applicable)
__pycache__/
*.py[cod]

# Ignore package manager caches and build artifacts
node_modules/
bower_components/
.cargo/
target/
vendor/

# Ignore application-specific settings or temporary files
settings.json
config.json
db.sqlite3
.env
